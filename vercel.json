{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "copy": [
    {
      "from": "dist/views",
      "to": "dist/views",
      "recursive": true
    },
    {
      "from": "public",
      "to": "dist/public"
    }
  ]
}
